{
  "name": "higlass-widget",
  "version": "0.0.0",
  "main": "src/widget.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:nb build:lab",
    "build:nb": "esbuild --bundle --format=esm --outdir=./higlass_widget/nbextension/ src/widget.js",
    "build:lab": "jupyter labextension build .",
    "clean": "run-p clean:nb clean:lab",
    "clean:nb": "rimraf higlass_widget/nbextension/widget.{js,css}",
    "clean:lab": "rimraf higlass_widget/labextension/static/ && rimraf higlass_widget/labextension/package.json",
    "copy-dist": "mkdir -p dist && cp higlass_widget/nbextension/* dist",
    "prepack": "run-s build:nb copy-dist"
  },
  "keywords": [],
  "jupyterlab": {
    "extension": "src/plugin",
    "outputDir": "higlass_widget/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "devDependencies": {
    "@jupyter-widgets/base": "^4.0.0",
    "@jupyterlab/builder": "^3.2.9",
    "css-loader": "^6.6.0",
    "esbuild": "^0.14.21",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1"
  },
  "dependencies": {
    "higlass": "^1.11.9",
    "npm-run-all": "^4.1.5",
    "pixi.js": "^6.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
